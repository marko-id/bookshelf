{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "arrow-body-style": "off",
        "indent": "off",
        "no-unused-vars": "off",
        "eol-last": "off",
        "max-len": "off",
        "quotes": "off"
    }
}
